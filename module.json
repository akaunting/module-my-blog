{
    "alias": "my-blog",
    "icon": "fa fa-pen",
    "version": "1.0.0",
    "active": 1,
    "providers": [
        "Modules\\MyBlog\\Providers\\Event",
        "Modules\\MyBlog\\Providers\\Main"
    ],
    "aliases": {},
    "files": [],
    "requires": [],
