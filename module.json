        "Modules\\MyBlog\\Widgets\\TotalAuthors",
        "Modules\\MyBlog\\Widgets\\TotalPosts",
        "Modules\\MyBlog\\Widgets\\TotalComments",
        "Modules\\MyBlog\\Widgets\\PostsByCategory",
